# Production Environment Variables for Vercel
# Copy these to Vercel Dashboard → Settings → Environment Variables

# Database (Get from Vercel Postgres after creation)
DATABASE_URL="postgresql://user:password@host:5432/database"

# Auth Secret (Keep this secure!)
AUTH_SECRET="local-dev-secret-key-change-me"

# Base URL (Update with your actual Vercel URL)
NEXT_PUBLIC_BASE_URL="https://ketteyos.vercel.app"
